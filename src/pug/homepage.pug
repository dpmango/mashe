    // ADVANTAGES
    section.advantages
      .container
        // ADVANTAGES      
        include components/_advantages
              
    .container
      h2.masterpiece__title.wow
        span Творите шедевры
      h3.masterpiece__subtitle.wow(data-animation-delay=".2s") Наслаждайтесь результатом
      .masterpiece__wrap
        a.masterpiece__item.masterpiece__item--photo.wow(href="#")
          .masterpiece__title-wrap
            h3.masterpiece__item-title Шелк для шитья
            span.masterpiece__item-amount 22 вида
              +icon('ico-wave-pink')
              +icon('ico-wave-pink')
            
          p.masterpiece__descr Возможна покраска шелка по pantone и тканипод заказ на выгодныхусловиях
        a.masterpiece__item.wow(href="#" data-animation-delay=".1s")
          .masterpiece__title-wrap
            h3.masterpiece__item-title Шелк для росписи
              +icon('ico-for-paint')
              //img(src="img/for-paint.svg")
            span.masterpiece__item-amount 25 видов
              +icon('ico-wave-pink')
              +icon('ico-wave-pink')
       
          p.masterpiece__descr Натуральный белый шелк, платки и шарфы
        a.masterpiece__item.masterpiece__item--photo.wow(href="#" data-animation-delay=".2s")
          .masterpiece__title-wrap
            h3.masterpiece__item-title Шелк для декора
            span.masterpiece__item-amount 22 вида
              +icon('ico-wave-pink')
              +icon('ico-wave-pink')
            
          p.masterpiece__descr Оптовые цены от 10 метров. Сотрудничество с фотогорафами и декораторами
        a.masterpiece__item.wow(href="#")
          .masterpiece__title-wrap
            h3.masterpiece__item-title Шелк для валяния
              +icon('ico-for-felt')
              //img(src="img/for-felt-w.svg")
            span.masterpiece__item-amount 25 видов
              +icon('ico-wave-pink')
              +icon('ico-wave-pink')
         
          p.masterpiece__descr Разреженный маргиланские шелка, лен, хлопок
        a.masterpiece__item.masterpiece__item--photo.wow(href="#" data-animation-delay=".1s")
          .masterpiece__title-wrap
            h3.masterpiece__item-title Шелк для вышивки
            span.masterpiece__item-amount 22 вида
              +icon('ico-wave-pink')
