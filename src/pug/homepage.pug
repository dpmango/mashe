extends shared/_layout
block content

  // MAIN SLIDER
  section.main-slider(jsMainSlider)
    .main-slider__slide.main-slider__slide--first
      .container
        span.main-slider__subtitle.wow Натуральный
        h1.main-slider__title.wow(data-animation-delay=".1s") Шелк для декора
        p.main-slider__descr.wow(data-animation-delay=".3s") от 315 рублей, с доставкой по России от 2 дней
        a.main-slider__btn(href="#" data-animation-delay=".6s").wow Перейти в каталог
    .main-slider__slide.main-slider__slide--second
      .container
        span.main-slider__subtitle Белоснежный
        h1.main-slider__title Шелк для росписи
        p.main-slider__descr от 315 рублей, с доставкой по России от 2 дней
        a.main-slider__btn(href="#") Перейти в каталог
        
            
